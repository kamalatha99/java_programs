import java.util.*;
class SUM
{
    public static void main(String args[])
    {
        Scanner obj=new Scanner(System.in);
        int n=obj.nextInt();
        for(int i=0;i<n;i++)
        {
            int a=obj.nextInt();
            int b=obj.nextInt();
            System.out.println(a+b);
        }
    }
}
